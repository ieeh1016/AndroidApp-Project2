<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#f1f3f5"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:orientation="vertical"
                android:layout_gravity="center">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:text="당신의 진단결과"
                    android:textSize="30dp"
                    android:fontFamily="@font/font2"
                    android:layout_gravity="center">

                </TextView>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/intj" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="65dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:text="유형:"
                            android:fontFamily="@font/main"
                            android:textStyle="bold"
                            android:textSize="15dp">

                        </TextView>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/main"
                            android:text="용의주도한 전략가"
                            android:textSize="15dp">
                        </TextView>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="65dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:text="MBTI:"
                            android:fontFamily="@font/main"
                            android:textStyle="bold"
                            android:textSize="15dp">
                        </TextView>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/main"
                            android:text="INTJ"
                            android:textSize="15dp">
                        </TextView>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="65dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:text="연예인:"
                            android:fontFamily="@font/main"
                            android:textStyle="bold"
                            android:textSize="15dp">
                        </TextView>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/main"
                            android:text="공민지 , 류진 , 보아 , 은혁"
                            android:textSize="15dp">
                        </TextView>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="65dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:text="비율:"
                            android:fontFamily="@font/main"
                            android:textStyle="bold"
                            android:textSize="15dp">
                        </TextView>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/main"
                            android:text="3.75%"
                            android:textSize="15dp">
                        </TextView>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/resultstyle1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/font2"
                        android:text="내향적"
                        android:textSize="15dp">

                    </TextView>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/inner"></ImageView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/resultstyle2"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/font2"
                        android:text="직관적"
                        android:textSize="15dp">

                    </TextView>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/intuition"></ImageView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/resultstyle3"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/font2"
                        android:text="사고적"
                        android:textSize="15dp">

                    </TextView>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/thinking"></ImageView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/resultstyle4"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/font2"
                        android:text="판단적"
                        android:textSize="15dp">

                    </TextView>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/judge"></ImageView>
                </LinearLayout>

            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/descriptionstyle"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:orientation="vertical">
                <TextView


                    android:layout_gravity="center"
                    android:layout_width="380dp"
                    android:layout_height="50dp"
                    android:text="💪INTJ 강점"
                    android:textSize="20dp"
                    android:fontFamily="@font/main"
                    android:textColor="@color/black"
                    android:gravity="center"></TextView>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:layout_marginBottom="13dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="380dp"
                    android:layout_height="100dp"

                    android:src="@drawable/intj_good"></ImageView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸합리적 – 건축가는 자신의 마음을 자랑스럽게 생각합니다. 그들에게는 거의 모든 상황이 지식을 확장하고 합리적 사고 능력을 연마할 수 있는 기회가 될 수 있습니다. 이러한 사고 방식 덕분에 그들은 가장 어려운 문제에도 독창적인 솔루션을 고안할 수 있습니다."></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸정보에 입각한 – 합리적이고 증거에 기반한 의견을 형성하는 데 건축가만큼 헌신적인 성격 유형은 거의 없습니다. 직감이나 반쯤 구어진 가정보다는 연구와 분석에 근거하여 결론을 내립니다. 이것은 그들에게 의견 불일치에 직면하더라도 자신의 아이디어를 옹호하는 데 필요한 자신감을 줍니다.
"></TextView>

            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸독립적 – 이러한 성격 유형의 경우 순응은 평범함과 거의 동의어입니다. 창의적이고 자기 의욕이 넘치는 건축가는 자신의 방식대로 일을 하려고 노력합니다. 그들은 규칙이나 관습이 성공을 가로막는 것을 허용하는 것보다 더 실망스러운 일을 상상할 수 있습니다."></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸결단력 - 건축가 성격은 야심차고 목표 지향적입니다. 아이디어나 추구가 상상력을 사로잡을 때마다 건축가는 주제를 마스터하고 관련 기술을 습득하는 데 전념합니다. 그들은 성공한다는 것이 무엇을 의미하는지에 대한 명확한 비전을 가지고 있는 경향이 있으며 이러한 비전을 현실로 바꾸는 것을 막을 수 있는 것은 거의 없습니다."></TextView>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸호기심 – 건축가는 새로운 아이디어에 열려 있습니다. 그 아이디어가 합리적이고 증거에 기반하는 한 말입니다. 본질적으로 회의적인 이 성격 유형은 특히 엉뚱하거나 반대되는 관점에 끌립니다. 그들은 사실이 자신이 틀렸음을 증명할 때 자신의 의견을 바꾸는 데 개방적입니다."></TextView>

            </LinearLayout>


            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/descriptionstyle"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:orientation="vertical">
                <TextView


                    android:layout_gravity="center"
                    android:layout_width="380dp"
                    android:layout_height="50dp"
                    android:text="💉INTJ 약점"
                    android:textSize="20dp"
                    android:fontFamily="@font/main"
                    android:textColor="@color/black"
                    android:gravity="center"></TextView>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:layout_marginBottom="13dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="380dp"
                    android:layout_height="100dp"

                    android:src="@drawable/intj_bad"></ImageView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸오만함 – 건축가는 지식이 풍부할 수 있지만 오류가 없는 것은 아닙니다. 그들의 자기 확신은 다른 사람들, 특히 지적으로 열등하다고 생각하는 사람의 유용한 정보에 눈을 멀게 할 수 있습니다. 이러한 성격은 또한 다른 사람들이 틀렸다는 것을 증명하려고 할 때 불필요하게 가혹하거나 편협해질 수 있습니다.

"></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸감정 거부 – 건축가에게 합리성은 왕입니다. 그러나 감정적 맥락은 종종 이러한 성격 유형을 가진 사람들이 인정하는 것보다 더 중요합니다. 건축가는 사실보다 감정을 중시하는 사람에게 조바심을 낼 수 있습니다. 불행히도 감정을 무시하는 것은 건축가의 판단을 흐리게 할 수 있는 일종의 편견입니다.
"></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸지나치게 비판적 - 이러한 성격은 특히 생각과 감정에 관해서 자제력이 강한 경향이 있습니다. 자신의 삶에 있는 사람들이 자신의 억제 수준에 맞지 않을 때 건축가는 매우 비판적일 수 있습니다. 그러나 이러한 비판은 인간 본성에 대한 완전한 이해라기보다는 자의적인 기준에 근거한 불공정한 경우가 많습니다."></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸전투적 – 건축가는 이유를 이해하지 못한 채 맹목적으로 무엇이든 따르는 것을 싫어 합니다. 여기에는 제한과 이를 부과하는 권위자가 포함됩니다. 이 성격 유형을 가진 사람들은 쓸모없는 규칙과 규정에 대해 논쟁하는 데 휘말릴 수 있습니다. 그러나 때때로 이러한 싸움은 더 중요한 문제에서 주의를 산만하게 합니다."></TextView>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸낭만주의적 단서 – 건축가의 집요한 합리성은 낭만주의에 좌절하게 만들 수 있습니다. 특히 관계의 초기 단계에서 그들은 무슨 일이 일어나고 있고 어떻게 행동해야 하는지 이해하는 데 어려움을 겪을 수 있습니다. 그리고 그들의 관계가 이해할 수 없는 이유로 무너지면 마음의 문제에 대해 냉소적이 되고 심지어 사랑과 연결의 중요성에 대해 의문을 제기할 수 있습니다."></TextView>

            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/descriptionstyle"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:orientation="vertical">
                <TextView


                    android:layout_gravity="center"
                    android:layout_width="380dp"
                    android:layout_height="50dp"
                    android:text="💖환상의 궁합"
                    android:textSize="20dp"
                    android:fontFamily="@font/main"
                    android:textColor="@color/black"
                    android:gravity="center"></TextView>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="500dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:layout_marginBottom="13dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="380dp"
                    android:layout_height="100dp"
                    android:src="@drawable/esfp"></ImageView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textSize="15dp"
                    android:layout_margin="10dp"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:fontFamily="@font/font2"
                    android:text="ESFP 자유로운 영혼의 연예인"></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="500dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:layout_marginBottom="13dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="380dp"
                    android:layout_height="100dp"
                    android:src="@drawable/estp"></ImageView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textSize="15dp"
                    android:layout_margin="10dp"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:fontFamily="@font/font2"
                    android:text="ESTP 모험을 즐기는 사업가"></TextView>

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>



</ScrollView>