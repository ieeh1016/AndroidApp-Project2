<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#f1f3f5"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:orientation="vertical"
                android:layout_gravity="center">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:text="당신의 진단결과"
                    android:textSize="30dp"
                    android:fontFamily="@font/font2"
                    android:layout_gravity="center">

                </TextView>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/istj" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="65dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:text="유형:"
                            android:fontFamily="@font/main"
                            android:textStyle="bold"
                            android:textSize="15dp">

                        </TextView>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/main"
                            android:text="청렴결백한 논리주의자"
                            android:textSize="15dp">
                        </TextView>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="65dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:text="MBTI:"
                            android:fontFamily="@font/main"
                            android:textStyle="bold"
                            android:textSize="15dp">
                        </TextView>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/main"
                            android:text="ISTJ"
                            android:textSize="15dp">
                        </TextView>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="65dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:text="연예인:"
                            android:fontFamily="@font/main"
                            android:textStyle="bold"
                            android:textSize="15dp">
                        </TextView>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/main"
                            android:text="써니 , 조현영 , 찬미 , 효연"
                            android:textSize="15dp">
                        </TextView>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="65dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:text="비율:"
                            android:fontFamily="@font/main"
                            android:textStyle="bold"
                            android:textSize="15dp">
                        </TextView>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/main"
                            android:text="4.28%"
                            android:textSize="15dp">
                        </TextView>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/resultstyle1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/font2"
                        android:text="내향적"
                        android:textSize="15dp">

                    </TextView>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/inner"></ImageView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/resultstyle2"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/font2"
                        android:text="관찰적"
                        android:textSize="15dp">

                    </TextView>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/observe"></ImageView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/resultstyle3"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/font2"
                        android:text="사고적"
                        android:textSize="15dp">

                    </TextView>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/thinking"></ImageView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/resultstyle4"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/font2"
                        android:text="인식적"
                        android:textSize="15dp">

                    </TextView>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/aware"></ImageView>
                </LinearLayout>

            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/descriptionstyle"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:orientation="vertical">
                <TextView


                    android:layout_gravity="center"
                    android:layout_width="380dp"
                    android:layout_height="50dp"
                    android:text="💪ISTJ 강점"
                    android:textSize="20dp"
                    android:fontFamily="@font/main"
                    android:textColor="@color/black"
                    android:gravity="center"></TextView>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:layout_marginBottom="13dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="380dp"
                    android:layout_height="100dp"

                    android:src="@drawable/istj_good"></ImageView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸정직하고 직접적 - 성실성은 물류인 성격 유형의 핵심입니다. 감정 조작, 마음의 게임, 안심시키는 거짓말은 모두 단순하고 단순한 정직으로 직면하는 상황의 현실을 관리하려는 물류 전문가의 선호도와 상반됩니다."></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸의지가 강하고 충실한 - 물류 전문가 는 자신의 행동에서도 이러한 성실성을 구현하고 열심히 일하고 목표에 집중합니다. 인내심 있고 결단력 있는 물류인 성격 유형을 가진 사람들은 의무를 다합니다.
"></TextView>

            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸매우 책임감 있는 – 물류인의 말은 약속이며 약속은 모든 것을 의미합니다. 물류 전문가들은 그들이 말했던 결과를 제공하지 못하느니 차라리 일을 더 하고 잠을 자지 않고 땅바닥에 뛰어들기를 원합니다. 충성심은 Logistician 성격에 대한 강한 감정이며 자신이 헌신한 사람과 조직에 대한 의무를 다합니다."></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸침착하고 실용적입니다. 물류 전문가가 화를 내고 어려움의 징후가 나타날 때마다 무너진다면 그들의 약속은 큰 의미가 없을 것입니다. 그들은 발을 땅에 붙이고 명확하고 합리적인 결정을 내립니다. 이 과정에서 사람들의 선호도를 고려해야 하며, 물류인들은 개인의 자질을 최대한 활용하기 위해 노력하지만, 이러한 결정은 공감보다 효율성을 염두에 두고 내려집니다. 다른 사람과 자신에 대한 비판에도 동일하게 적용됩니다."></TextView>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸질서 생성 및 집행 – 모든 물류 전문가 의 주요 목표는 자신이 선택한 일을 효과적으로 수행하는 것이며, 관련된 모든 사람이 무슨 일이 왜 일어나고 있는지 정확히 알고 있을 때 가장 잘 달성된다고 믿습니다. 불분명한 지침과 확립된 규칙을 어기는 사람들은 이러한 노력을 약화시키며 물류 전문가들은 거의 용인하지 않습니다. 구조와 규칙은 신뢰성을 촉진합니다. 혼돈은 예상치 못한 차질과 마감 기한을 놓치게 만듭니다."></TextView>

            </LinearLayout>


            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/descriptionstyle"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:orientation="vertical">
                <TextView


                    android:layout_gravity="center"
                    android:layout_width="380dp"
                    android:layout_height="50dp"
                    android:text="💉ISTJ 약점"
                    android:textSize="20dp"
                    android:fontFamily="@font/main"
                    android:textColor="@color/black"
                    android:gravity="center"></TextView>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:layout_marginBottom="13dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="380dp"
                    android:layout_height="100dp"

                    android:src="@drawable/istj_bad"></ImageView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸완고함 – 사실은 사실이며 물류 전문가는 자신이 뒷받침하지 않는 새로운 아이디어에 저항하는 경향이 있습니다. 이 사실에 입각한 의사 결정 과정은 또한 로지스틱스 성격 유형을 가진 사람들이 자신이 무언가에 대해 잘못되었다는 것을 받아들이는 것을 어렵게 만듭니다. 그러나 누구나 세부 사항을 놓칠 수 있습니다.

"></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸둔감함 – 의도적으로 가혹한 것은 아니지만 물류 전문가는 정직이 최선의 정책이라는 단순한 주문으로 더 민감한 유형의 감정을 상하게 하는 경우가 많습니다. 물류 전문가는 감정을 고려할 수 있지만 실제로는 말해야 할 것을 말하는 가장 효과적인 방법을 결정할 때까지만 가능합니다.
"></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸항상 책에 의해 – 물류 전문가는 명확하게 정의된 규칙이 있으면 가장 잘 작동한다고 생각하지만, 이로 인해 단점이 최소화된 경우에도 규칙을 어기거나 새로운 것을 시도하는 것을 꺼립니다. 실제로 구조화되지 않은 환경은 물류 전문가를 거의 마비 상태로 만듭니다."></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸판단력 – 의견은 의견이고 사실은 사실이며, 물류 전문가는 이러한 사실에 동의하지 않는 사람, 특히 고의적으로 무시하는 사람을 존중하지 않을 것입니다."></TextView>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textSize="13dp"
                    android:text="🔸종종 부당하게 자신을 비난함 – 이 모든 것이 결합되어 물류 담당자가 프로젝트를 안정적으로 볼 수 있는 유일한 사람이라고 믿게 만들 수 있습니다. 추가 작업과 책임을 지고 좋은 의도와 유용한 아이디어를 외면하면서 물류 전문가는 조만간 단순히 전달할 수 없는 전환점에 도달합니다. 스스로 책임을 져야 했기 때문에 물류 전문가는 실패에 대한 책임이 자신에게만 있다고 생각합니다."></TextView>

            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/descriptionstyle"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:orientation="vertical">
                <TextView


                    android:layout_gravity="center"
                    android:layout_width="380dp"
                    android:layout_height="50dp"
                    android:text="💖환상의 궁합"
                    android:textSize="20dp"
                    android:fontFamily="@font/main"
                    android:textColor="@color/black"
                    android:gravity="center"></TextView>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="500dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:layout_marginBottom="13dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="380dp"
                    android:layout_height="100dp"
                    android:src="@drawable/esfp"></ImageView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textSize="15dp"
                    android:layout_margin="10dp"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:fontFamily="@font/font2"
                    android:text="ESFP 선의의 옹호자"></TextView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="13dp"
                android:layout_width="500dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:layout_marginBottom="13dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="380dp"
                    android:layout_height="100dp"
                    android:src="@drawable/estp"></ImageView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textSize="15dp"
                    android:layout_margin="10dp"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:fontFamily="@font/font2"
                    android:text="ESTP 용의주도한 전략가"></TextView>

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>



</ScrollView>